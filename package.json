{
  "dependencies": {
    "esm": "^3.2.25",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "eslint-plugin-prettier": "^5.1.1",
    "mocha": "^10.2.0",
    "standard": "^17.1.0"
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "prettier/standard"
    ],
    "rules": {
      "no-trailing-spaces": "off",
      "no-multiple-empty-lines": "off",
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --require esm",
    "dev": "node --watch --trace-warnings src/index.js"
  }
}
