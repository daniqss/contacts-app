{
  "type": "module",
  "dependencies": {
    "apicache": "^1.6.3",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "mongodb": "^6.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "eslint-plugin-prettier": "^5.1.1",
    "mocha": "^10.2.0",
    "standard": "^17.1.0",
    "supertest": "^6.3.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "printWidth": 0,
    "semi": false,
    "tabWidth": 4,
    "bracketSameLine": true
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "no-trailing-spaces": "off",
      "no-multiple-empty-lines": "off",
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "scripts": {
    "test": "mocha --require esm --spec test/**/*.test.js",
    "dev": "node --watch --trace-warnings src/index.js"
  }
}
